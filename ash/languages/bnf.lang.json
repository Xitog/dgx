{
    "name": "bnf",
    "definitions": {
        "non-terminal": ["<[\\w- ]+>"],
        "identifier": ["expansion", "A", "B", "C", "D", "nom"],
        "operator": ["::=", "\\|", "\\.\\.\\.", "=", "-", "\\?", "\\*", "\\+", "@", "\\$", "_"],
        "separator": ["\\(", "\\)", "\\[", "\\]", "\\{", "\\}", ",", ";"],
        "terminal" : ["\"[\\w- <>:=,;\\|']*\"", "'[\\w- <>:=,;\\|\"]*'"],
        "blank": ["[ \u00A0\\t]+"],
        "comment": ["#[^\n]*\n"],
        "newline" : ["\n", "\n\r", "\r\n"]
    }
}
